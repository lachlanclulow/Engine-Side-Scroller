import org.newdawn.slick.Image;

public abstract class Entity {
	
	private double x;
	private double y;
	
	private Image sprite;
	
	public double getX() {
		return x;
	}
	public void setX(double x) {
		this.x = x;
	}
	public double getY(){
		return y;
	}
	public void setY(double y);
	public abstract void render();
	public abstract void update();
}
